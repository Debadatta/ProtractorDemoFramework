{
    "homePage":
    {
      "homePageLogo": ["xpath", "//h2[contains(text(),'AngularJS')]"],
      "yourNameTextBox": ["model","yourName"],
      "heading": ["xpath","//h1[@class='ng-binding']"],
      "toDoTextBox": ["model","todoList.todoText"],
      "addButton": ["xpath","//input[@value='add']"],
      "learnLink": ["xpath", "//a[contains(text(),'Learn')]"],
      "checkBoxLabel": ["xpath","//form[@ng-submit='todoList.addTodo()']/parent::div/child::ul/li[last()]/label/span"]
      
    },

    "tutorialPage": {
      "tutorialClassName": ["class", "ng-binding"]
    }
}